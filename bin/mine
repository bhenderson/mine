#!/bin/bash

# writable file needed to source from function
[[ -a "/dev/fd/3" ]] || exec 3>/dev/null

# add libs
export PATH="$mine_path/lib:$PATH"
source 'common.sh'
export command_name=mine

case "$1" in
  use)
    shift
    source 'use.sh' "$@"
  ;;
  list)
    shift
    source 'list.sh' "$@"
  ;;
esac

update_ruby

# vim: ft=sh :
